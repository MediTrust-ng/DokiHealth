{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowNetworkAccessThroughNAT",
      "Effect": "Allow",
      "Action": [
        "ec2:DescribeNetworkInterfaces",
        "ec2:CreateNetworkInterface",
        "ec2:DeleteNetworkInterface"
      ],
      "Resource": "*"
    },
    {
      "Sid": "AllowSupabaseInteraction",
      "Effect": "Allow",
      "Action": [
        "execute-api:Invoke"
      ],
      "Resource": "*"
    }
  ]
}
